<div class="-space-x-6 avatar-group flex-wrap justify-evenly w-64 p-3">
  <% users.each do |user| %>
    <a href="<%= user_path(user.id) %>">
    <div class="avatar">
      <div class="w-16 h-16">
        <% if user.image.attached? %>
          <%= image_tag user.image, width: 256, class: 'inline-block h-14 w-14 rounded-full object-cover' %>
        <% else %>
          <%= image_tag 'https://cdn.pixabay.com/photo/2016/08/08/09/17/avatar-1577909_960_720.png' %>
        <% end %>
      </div>
    </div>
    </a>
  <% end %>
</div>